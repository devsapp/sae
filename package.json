{
  "name": "sae",
  "version": "0.0.1",
  "description": "This is a component demo for Serverless Devs Tool ",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "autoInstall": false,
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w && npm run doc",
    "prebuild": "tsc",
    "prepublish": "npm run build",
    "publish": "s cli platform publish",
    "build": "npx ncc cache clean && npx ncc build lib/index.js -o lib",
    "postbuild": "npm run doc",
    "doc": " npx typedoc src/index.ts --json doc/doc.json --out doc"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.10",
    "@serverless-devs/core": "^0.0.*",
    "i18n": "^0.13.2",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "tty-table": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7"
  }
}
